{
  "name": "react-boilerplate",
  "version": "1.2.0",
  "main": "src/index.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/deemoding/React-boilerplate"
  },
  "scripts": {
    "precommit": "npm run lint",
    "commitmsg": "npm run lint:msg",
    "lint": "eslint --cache --fix --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "start": "webpack-dev-server --progress",
    "build": "webpack --mode production --config webpack.production.config.js",
    "validate": "npm ls",
    "cz": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "antd": "^3.16.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "showdown": "^1.9.0",
    "@ant-design/icons": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^1.0.0",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "~2.0.11",
    "copy-webpack-plugin": "~4.6.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "~2.12.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "~7.9.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "no-console-webpack-plugin": "0.0.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "raw-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "keywords": [
    "es6",
    "web app",
    "react"
  ],
  "author": "DeemoDing",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
