{
  "name": "React-boilerplate",
  "version": "1.1.0",
  "main": "public/main.js",
  "scripts": {
    "precommit": "npm run lint",
    "commitmsg": "npm run lint:msg",
    "lint": "eslint --cache --fix --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "start": "webpack-dev-server --mode development --devtool eval --progress",
    "build": "webpack --mode production --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "antd": "~3.6.3",
    "immutable": "^3.7.4",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "~16.4.1",
    "showdown": "^1.8.6"
  },
  "devDependencies": {
    "babel-core": "~6.26.3",
    "babel-eslint": "~8.2.3",
    "babel-loader": "~7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "~2.0.1",
    "copy-webpack-plugin": "~4.5.1",
    "css-loader": "~0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "~2.12.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "~7.9.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "no-console-webpack-plugin": "0.0.5",
    "node-sass": "^4.9.0",
    "open-browser-webpack-plugin": "0.0.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "style-loader": "~0.21.0",
    "url-loader": "^1.0.1",
    "validate-commit-msg": "~2.14.0",
    "webpack": "^4.6.0",
    "webpack-cli": "~3.0.8",
    "webpack-dev-server": "^3.1.3",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "keywords": [
    "es6",
    "web app"
  ],
  "author": "DeemoDing",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
